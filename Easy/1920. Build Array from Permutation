Question Link - https://leetcode.com/problems/build-array-from-permutation/


class Solution {
    public int[] buildArray(int[] nums) {
        final int n = nums.length;
        int [] arr= new int[n];
        for(int i = 0;i<n;i++){
            arr[i]= nums[nums[i]];
        }
    return arr;
    }
}
